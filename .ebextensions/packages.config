packages:
    yum:
        amazon-linux-extras: []
        jq: []

commands:
    01_postgres_activate:
        command: sudo amazon-linux-extras enable postgresql13
    02_postgres_install:
        command: sudo yum install -y postgresql-devel
    setvars:
        command: /opt/elasticbeanstalk/bin/get-config environment | jq -r 'to_entries | .[] | "export \(.key)=\"\(.value)\""' > /etc/pro